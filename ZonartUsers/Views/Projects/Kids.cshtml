    </section>

    <section style="display: inline-flex">
        <a class="mb-1 mt-1 mr-2 pr-3 pl-3 pb-2 pt-2" asp-area="" asp-controller="Projects" asp-action="Variety" id="variety" style="color: white">Variety</a>
    </section>

</div>


<div>
    <img src="../Images/projects/kids/kids.jpg" style="max-width: 100%; height: auto; z-index: -1" />
</div>



<script>
    $(document).ready(function () {
        $(".active").removeClass("active");
        $("#projects").addClass("active");
    });
</script>



