@{
    ViewData["Title"] = "Kids";
}


<div class="container text-center my-projects" style="background-color:#949598; padding-top:100px; font-family:Jost; padding-bottom:50px">

    <section style="display: inline-flex">
        <a class="mb-1 mb-1 mr-2 pr-3 pl-3 pb-2 pt-2" asp-area="" asp-controller="Projects" asp-action="Brochures" id="brochures" style="color: white; ">Brochures</a>
    </section>

    <section style="display: inline-flex">
        <a class="mb-1 mt-1 mr-2 pr-3 pl-3 pb-2 pt-2" asp-area="" asp-controller="Projects" asp-action="Logos" id="logos" style="color: white">Logos</a>
    </section>

    <section style="display: inline-flex">
        <a class="mb-1 mt-1 mr-2 pr-3 pl-3 pb-2 pt-2" asp-area="" asp-controller="Projects" asp-action="Books" id="books" style="color: white">Books</a>
    </section>

    <section style="display: inline-flex">
        <a class="mb-1 mt-1 mr-2 pr-3 pl-3 pb-2 pt-2" asp-area="" asp-controller="Projects" asp-action="Posters" id="posters" style="color: white">Posters</a>
